<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="361.0" prefWidth="514.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.haliimo.patientdatabase.gui.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Button fx:id="delete" mnemonicParsing="false" onAction="#onDeleteClick" text="Delete" />
      <TableView prefHeight="200.0" prefWidth="200.0" fx:id="tablePatients">
        <columns>
          <TableColumn fx:id="columnId" prefWidth="61.0" text="ID" />
          <TableColumn fx:id="columnName" prefWidth="123.0" text="Name" />
            <TableColumn fx:id="columnEmail" prefWidth="175.0" text="Email" />
            <TableColumn fx:id="columnDiagnosis" prefWidth="114.0" text="Diagnosis" />
        </columns>
      </TableView>
   </children>
</VBox>
